mark
   skbeg     -2  +1  -1
      linseg -2  -1  -1
      linseg -2  -1  +1
      linseg -2  +1  +1
      linseg -2  +1  -1
   skend 1

   skbeg     +2  +1  -2
      linseg +2  -1  -2
      linseg +2  -1  +2
      linseg +2  +1  +2
      linseg +2  +1  -2
   skend 1
loft 1

end
