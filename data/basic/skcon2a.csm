despmtr   xbase     0.1
despmtr   ybase     0.2
despmtr   zbase     0.3
despmtr   width     3.0
despmtr   height    2.0
despmtr   thick     0.5

skbeg     xbase  ybase  zbase
   skvar  xy  " 0.1;  0.2;  0.0;\
                4.0;  0.0;  0.0;\
                4.0;  2.0;  0.0;\
                2.1;  1.9;  0.0;\
                2.0;  3.8;  0.0;\
                0.0;  4.0;  0.0;"

   linseg ::x[2]  ::y[2]  ::z[2]
   linseg ::x[3]  ::y[3]  ::z[3]
   linseg ::x[4]  ::y[4]  ::z[4]
   linseg ::x[5]  ::y[5]  ::z[5]
   linseg ::x[6]  ::y[6]  ::z[6]
   linseg ::x[1]  ::y[1]  ::z[1]
skend

end
