#

all:
	forfiles /m *Writer /c "cmd /c if @isdir==TRUE echo Build @file & cd @file & nmake -f NMakefile"

clean:
	forfiles /m *Writer /c "cmd /c if @isdir==TRUE echo Clean @file & cd @file & nmake -f NMakefile clean"

cleanall:
	forfiles /m *Writer /c "cmd /c if @isdir==TRUE echo Cleanall @file & cd @file & nmake -f NMakefile cleanall"
